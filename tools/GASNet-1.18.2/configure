if test "${enable_backtrace_execinfo+set}" = set; then :
  enableval=$enable_backtrace_execinfo;
fi

# Check whether --enable-backtrace-execinfo was given.
if test "${enable_backtrace_execinfo+set}" = set; then :
  enableval=$enable_backtrace_execinfo;
fi

