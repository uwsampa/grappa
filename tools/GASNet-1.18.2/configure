# vvvvvvvvvvvvvvvvvvvvvv GASNET_PUSHVAR(CFLAGS,"$CFLAGS $MISC_CPPFLAGS $MISC_CFLAGS -I$TOP_SRCDIR/other") vvvvvvvvvvvvvvvvvvvvvv (L:1)

    if test "$_pushcnt_CFLAGS" = "" ; then
